{
    "name": "zippy-lang",
    "displayName": "Zippy Language",
    "description": "The Zippy Programming Language",
    "publisher": "zipdot",
    "version": "0.3.0",
    "icon": "icon.png",
    "license": "MIT",
    "keywords": [
        "zippy",
        "syntax"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/zippy-lang/zippy-vscode"
    },
    "engines": {
        "vscode": "^1.38.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "zippy",
            "aliases": ["Zippy", "zippy"],
            "extensions": [".zip"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "zippy",
            "scopeName": "source.zip",
            "path": "./syntaxes/zippy.tmLanguage.json"
        }]
    }
}
